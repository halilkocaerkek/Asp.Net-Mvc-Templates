@model DataModel.belt

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Details</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("List", "Index")
            </li>
            <li class="active">
                <strong>Details</strong>
            </li>
        </ol>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Details belt</h5>
                </div>
                <div class="ibox-content">


				    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.beltColor.name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.beltColor.name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.branch.name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.branch.name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.order)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.order)
        </dd>

    </dl>


	    @Html.ActionLink("Edit", "Edit", new { id = Model.id }, new { @class = "btn btn-primary"})
    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-white"})

                </div>
            </div>
        </div>
    </div>
 </div>


 <div class="ibox">
    <div class="ibox-title">
        <h5>Details belt</h5>
    </div>
    <div class="ibox-content">

        <div class="panel-group payments-method" id="accordion">


            
 

             <div class="panel panel-default">
                @{
                    string panelid = "collapse" + @Html.DisplayNameFor(model => model.beltColor.name);
                    string panelref = "#collapse" +  @Html.DisplayNameFor(model => model.beltColor.name);
                    string panelname = ""+  @Html.DisplayNameFor(model => model.beltColor.name);
                }

                <div class="panel-heading">
                    <div class="pull-right">
                        <i class=" text-success"></i> <!-- icon -->
                    </div>
                    <h5 class="panel-title">

                        <a data-toggle="collapse" data-parent="#accordion" href="@panelref">@panelname</a>
                    </h5>
                </div>
                <div id="@panelid" class="panel-collapse collapse">
                    <div class="panel-body">
                        <div class="row">
                            
                           

                        </div>
                    </div>
                </div>
            </div>




           
 

             <div class="panel panel-default">
                @{
                    string panelid = "collapse" + @Html.DisplayNameFor(model => model.branch.name);
                    string panelref = "#collapse" +  @Html.DisplayNameFor(model => model.branch.name);
                    string panelname = ""+  @Html.DisplayNameFor(model => model.branch.name);
                }

                <div class="panel-heading">
                    <div class="pull-right">
                        <i class=" text-success"></i> <!-- icon -->
                    </div>
                    <h5 class="panel-title">

                        <a data-toggle="collapse" data-parent="#accordion" href="@panelref">@panelname</a>
                    </h5>
                </div>
                <div id="@panelid" class="panel-collapse collapse">
                    <div class="panel-body">
                        <div class="row">
                            
                           

                        </div>
                    </div>
                </div>
            </div>






        </div>

    </div>

</div>










 

